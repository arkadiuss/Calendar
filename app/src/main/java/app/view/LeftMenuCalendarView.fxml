<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox fx:id="leftVbox" prefHeight="516.0" prefWidth="284.0" spacing="80.0"
      xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="app.presenter.LeftMenuCalendarPresenter">
    <children>
        <ListView fx:id="calendarsList" prefHeight="314.0" prefWidth="218.0"
                  style="-fx-background-color: #AEADAD;">
            <VBox.margin>
                <Insets/>
            </VBox.margin>
        </ListView>
        <VBox prefHeight="200.0" prefWidth="100.0" spacing="25.0">
            <children>
                <Label text="Type calendar name" textFill="WHITE">
                    <padding>
                        <Insets left="35.0"/>
                    </padding>
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                    <VBox.margin>
                        <Insets bottom="-20.0"/>
                    </VBox.margin>
                </Label>
                <TextField fx:id="newCalendarName" alignment="CENTER" style="-fx-background-color: #AEADAD;"
                           text="&lt;&lt;calendar name&gt;&gt;">
                    <VBox.margin>
                        <Insets/>
                    </VBox.margin>
                </TextField>
                <Button fx:id="addCalendarButton" alignment="CENTER" mnemonicParsing="false"
                        onAction="#handleAddNewCalendar" style="-fx-background-color: #AEADAD;" text="Add"
                        textAlignment="CENTER">
                    <opaqueInsets>
                        <Insets left="50.0"/>
                    </opaqueInsets>
                    <VBox.margin>
                        <Insets left="75.0"/>
                    </VBox.margin>
                    <font>
                        <Font name="System Bold" size="13.0"/>
                    </font>
                </Button>
            </children>
        </VBox>
    </children>
</VBox>